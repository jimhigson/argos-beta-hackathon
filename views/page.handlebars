<!doctype html>
<html lang="en">

<head>

    <meta charset="UTF-8">

    <title>Argos beta</title>

    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/reset.css">

</head>

<body>

    <div id="trim"></div>

    <header id="hero">

        <div id="logo">
            <img src="/img/logo-a.png" alt="Googos">
        </div>

    </header>

    <section id="search">

        <div id="searchInner">

            <img class="searchIcon" width="32" height="32" src="/img/icon-search.png" alt="Search">
            
            <input type="text" class="mainSearch" placeholder="What are you looking for today?" value="{{startTerm}}">
            <input type="text" class="storeSearch" placeholder="at any store" value="">
            <div id="storesAutocomplete"></div>
        </div>
    </section>

    <div id="categories"></div>   
    <div id="results"></div>   
  

<script src="/bower_components/jquery/dist/jquery.js"></script>
<script src="/bower_components/unveil/jquery.unveil.min.js"></script>
<script src="/bower_components/sticky/jquery.sticky.js"></script>
<script src="/js/scripts.js"></script>

</body>

</html>
