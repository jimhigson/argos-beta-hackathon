<!doctype html>
<html lang="en">

<head>

    <meta charset="UTF-8">

    <title>Argos beta</title>

    <link rel="stylesheet" href="/css/style.css">

</head>

<body>

<div id="wrapper">

    <div id="trim"></div>

    <header id="hero">

        <div id="logo">
            <img src="/img/logo-a.png" alt="Googos">
        </div>

    </header>

    <section id="search">

        <div id="searchInner">
            <div class="searchIconWrap">
                <img width="32" height="32" src="/img/icon-search.png" alt="Search">
            </div>
            
            <input type="text" class="main-search" placeholder="What are you looking for today?" value="{{startTerm}}">
            
        </div>
    </section>

    <div id="categories"></div>   
    <div id="results"></div>   

</div>


<script src="/bower_components/jquery/dist/jquery.js"></script>
<script src="/bower_components/unveil/jquery.unveil.min.js"></script>
<script src="/bower_components/sticky/jquery.sticky.js"></script>
<script src="/js/scripts.js"></script>

</body>

</html>
